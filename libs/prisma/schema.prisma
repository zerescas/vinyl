datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./client"
}

model User {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
}

model Song {
  id     Int    @id @default(autoincrement())
  name   String
  length Float
}
